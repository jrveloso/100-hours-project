      <!-- Mobile scoreboard -->
      <li tabindex="0">
        <span class="active:bg-gray-300">
          <svg class="w-5" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"><!--! Font Awesome Pro 6.2.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2022 Fonticons, Inc. --><path d="M128 0c17.7 0 32 14.3 32 32V64H288V32c0-17.7 14.3-32 32-32s32 14.3 32 32V64h48c26.5 0 48 21.5 48 48v48H0V112C0 85.5 21.5 64 48 64H96V32c0-17.7 14.3-32 32-32zM0 192H448V464c0 26.5-21.5 48-48 48H48c-26.5 0-48-21.5-48-48V192zm80 64c-8.8 0-16 7.2-16 16v96c0 8.8 7.2 16 16 16h96c8.8 0 16-7.2 16-16V272c0-8.8-7.2-16-16-16H80z"/></svg>
          <span class="hidden md:visible">Scoreboard</span>
        </span>
        <ul class="rounded-box mb-5 gap-2 p-2 bg-gray-300 text-sm">
          <% if(gameInfo.scoreboard.games.length > 0) { %>
          <% for(var i=0; i<gameInfo.scoreboard.games.length; i++) { %>
          <div class="transform card w-100 bg-white shadow-xl transition duration-300 hover:scale-105">
            <div class="card-body p-3">
              <a class="w-100 flex flex-row gap-1" href="/boxscore/<%= gameInfo.scoreboard.games[i].gameId %>">
                <div class="border-solid border-r-2 border-gray-200">
                  <div class="flex justify-between gap-20 items-center">
                    <!-- Away Logo -->
                    <div class="flex gap-1 items-center">
                      <% if(gameInfo.scoreboard.games[i].awayTeam.teamTricode === 'BKN') {%>
                      <img src="https://cdn.ssref.net/req/202209231/tlogo/bbr/NJN.png" alt="<%= gameInfo.scoreboard.games[i].awayTeam.teamName %> Logo" class="h-7">
                      <% } else if(gameInfo.scoreboard.games[i].awayTeam.teamTricode === 'PHX') {%>
                      <img src="https://cdn.ssref.net/req/202209231/tlogo/bbr/PHO.png" alt="<%= gameInfo.scoreboard.games[i].awayTeam.teamName %> Logo" class="h-7">
                      <% } else if(gameInfo.scoreboard.games[i].awayTeam.teamTricode === 'NOP') {%>
                      <img src="https://cdn.ssref.net/req/202209231/tlogo/bbr/NOH.png" alt="<%= gameInfo.scoreboard.games[i].awayTeam.teamName %> Logo" class="h-7">
                      <% } else { %>
                      <img src="https://cdn.ssref.net/req/202209231/tlogo/bbr/<%= gameInfo.scoreboard.games[i].awayTeam.teamTricode %>.png" alt="<%= gameInfo.scoreboard.games[i].awayTeam.teamName %> Logo" class="h-7">
                      <% } %>
                      <span class="flex gap-1 flex-col"><%= gameInfo.scoreboard.games[i].awayTeam.teamTricode %></span>
                    </div>
                    <span class="justify-self-center pr-1"><%= gameInfo.scoreboard.games[i].awayTeam.score %></span>
                  </div>
                  <div class="flex justify-between gap-20 items-center">
                    <!-- Home Logo -->
                    <div class="flex gap-1 items-center">
                      <% if(gameInfo.scoreboard.games[i].homeTeam.teamTricode === 'BKN') {%>
                      <img src="https://cdn.ssref.net/req/202209231/tlogo/bbr/NJN.png" alt="<%= gameInfo.scoreboard.games[i].homeTeam.teamName %> Logo" class="h-7">
                      <% } else if(gameInfo.scoreboard.games[i].homeTeam.teamTricode === 'PHX') {%>
                      <img src="https://cdn.ssref.net/req/202209231/tlogo/bbr/PHO.png" alt="<%= gameInfo.scoreboard.games[i].homeTeam.teamName %> Logo" class="h-7">
                      <% } else if(gameInfo.scoreboard.games[i].homeTeam.teamTricode === 'NOP') {%>
                      <img src="https://cdn.ssref.net/req/202209231/tlogo/bbr/NOH.png" alt="<%= gameInfo.scoreboard.games[i].homeTeam.teamName %> Logo" class="h-7">
                      <% } else {%>
                      <img src="https://cdn.ssref.net/req/202209231/tlogo/bbr/<%= gameInfo.scoreboard.games[i].homeTeam.teamTricode %>.png" alt="<%= gameInfo.scoreboard.games[i].homeTeam.teamName %> Logo" class="h-7">
                      <% } %>
                      <span class="flex gap-1 flex-col"><%= gameInfo.scoreboard.games[i].homeTeam.teamTricode %></span>
                    </div>
                    <span class="justify-self-center pr-1"><%= gameInfo.scoreboard.games[i].homeTeam.score %></span>
                  </div>
                </div>
                <!-- Game Start/Time Remaining -->
                <% if(time.valueOf() >= Date.parse(gameInfo.scoreboard.games[i].gameTimeUTC)) { %>
                <span class="flex items-center text-center">
                  <%= time.valueOf() >= Date.parse(gameInfo.scoreboard.games[i].gameTimeUTC) ? gameInfo.scoreboard.games[i].gameStatusText : (Number(gameInfo.scoreboard.games[i].gameEt.slice(11, 13)) - 12 + gameInfo.scoreboard.games[i].gameEt.slice(13, 16)) + 'pm E.T.' %>
                </span>
                <% } else { %>
                <span class="flex items-center text-center">
                  <%= time.valueOf() >= Date.parse(gameInfo.scoreboard.games[i].gameTimeUTC) ? gameInfo.scoreboard.games[i].gameStatusText : (Number(gameInfo.scoreboard.games[i].gameEt.slice(11, 13)) - 12 + gameInfo.scoreboard.games[i].gameEt.slice(13, 16)) + 'pm E.T.' %>
                </span>
                <% } %>
              </a>
            </div>
          </div>
          <% } %>
          <% } %>
        </ul>
      </li>
      <!-- Watchlist -->
      <li tabindex="0">
